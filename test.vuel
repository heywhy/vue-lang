class App {
  App(name) {
    this.name = name;
  }

  call(name) {
    print "app calls " + this.name;
  }
}

class Circle < App {
  Circle(radius) {
    super("heywhy");
    this.radius = radius;
  }

  call() {
    super.call("me");
    return "lover";
  }
}


var circle: Circle = Circle(4);
print circle.call();
